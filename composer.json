{
    "name": "arcanesoft/arcanesoft",
    "description": "The Laravel Framework.",
    "type": "project",
    "license": "MIT",
    "keywords": ["arcanesoft", "framework", "laravel"],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "arcanesoft/analytics": "@dev",
        "arcanesoft/backups": "@dev",
        "arcanesoft/blog": "@dev",
        "arcanesoft/foundation": "@dev",
        "arcanesoft/media": "@dev",
        "arcanesoft/passport": "@dev",
        "arcanesoft/seo": "@dev",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.51",
        "laravel/tinker": "^2.5",
        "modules/account": "@dev",
        "modules/auth": "@dev",
        "modules/api": "@dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "modules/demo": "@dev",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./modules/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "Arcanesoft\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan arcanesoft:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
