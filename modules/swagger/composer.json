{
    "name":        "arcanedev/laravel-swagger",
    "description": "Swagger for laravel",
    "keywords":    ["arcanedev", "swagger", "api", "laravel"],
    "license":     "MIT",
    "type":        "project",
    "authors":     [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "arcanedev/support": "^8.0",
        "swagger-api/swagger-ui": "^3.45",
        "zircote/swagger-php": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"

    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Swagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Swagger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\Swagger\\SwaggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
