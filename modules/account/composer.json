    "keywords":    ["arcanedev", "arcanesoft", "user", "account", "laravel"],
    "license":     "MIT",
    "type":        "project",
    "authors":     [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
